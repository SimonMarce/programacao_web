    	return this.idade ++
    }, 
    altura : 1.91,
    mostrAltura : function(){
        console.log("Olá, eu tenho " + this.altura + " Metros")
    },
    peso : 70,
    mostrarPeso : function(){
        console.log("Olá! Meu nome é " + this.peso + " Kg")
    },
    andando : false, // variavel booleana
    metrosCaminhados : 0, // 0 como padrão
    andar : function(){
        metros : 10
        andando : true
        console.log("Andando : " + true ) 
        return this.metrosCaminhados + metros++
    },
    parar : function(){
        console.log("Andando : " + false)
    }
}

