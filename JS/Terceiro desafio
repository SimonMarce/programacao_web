var pessoa = {
    nome : "Irving",
    sobrenome : "Cesar",
    sexo : "Masculino",
    idade : 17,
    data: function fazerAniversario(){  //Método
    	return this.idade ++
    }, 
    altura : 1.91,
    peso : 70,
    andando : false, // variavel booleana
    metrosCaminhados : 0, // 0 como padrão
    andar : function(){
        metros : 10
        andando : true
        console.log("Andando : " + true ) 
        return this.metrosCaminhados + metros++
    }
}

undefined
pessoa.andar()
VM6118:16 Andando : true
63
pessoa.andar()
VM6118:16 Andando : true
64
pessoa.andar()
VM6118:16 Andando : true
65

