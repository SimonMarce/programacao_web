//5)
var teams = [undefined,'Flamengo',' Fluminese',' Boa vista',' Resende',' Bota Fogo.']

function ShowTeamPosition(num){
    if(num<=5){
        teams[num]
        console.log('O time que está em ' + num + '° lugar é ' + teams[num])
    }else{
        console.log('Não temos a informção do time que está nessa posição')
    }
}

//6)
ShowTeamPosition(1)
// O time que está em 1° lugar é Flamengo
ShowTeamPosition(2)
// O time que está em 2° lugar é  Fluminese
ShowTeamPosition(3)
// O time que está em 3° lugar é  Boa vista
ShowTeamPosition(4)
// O time que está em 4° lugar é  Resende


//7)
